PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX ogcgs: <http://www.opengis.net/ont/geosparql#>
PREFIX ogcgml: <http://www.opengis.net/ont/gml#>
PREFIX schema: <http://schema.org/>
PREFIX ruian: <https://ruian.linked.opendata.cz/slovnÃ­k/>
construct {?s ?p ?o}
where
    {
        ?s ?p ?o.
        ?s rdf:type ogcgs:Geometry .
        ?s rdf:type ruian:Okres .
        FILTER (?p = ogcgs:asGML || ?p = ogcgs:asWKT || ?p = ogcgml:id || ?p = schema:name)
    }
