PREFIX ddo: <http://onto.fel.cvut.cz/ontologies/dataset-descriptor/>
SELECT ?datasetSourceIri ?realType ?endpointUrl ?graphIri ?downloadUrl
WHERE {
    ?datasetSourceIri a ?type .
    OPTIONAL {
        ?datasetSourceIri ddo:has-endpoint-url ?endpointUrl .
        OPTIONAL {
            ?datasetSourceIri ddo:has-graph-id ?graphIri .
        }
    }
    OPTIONAL {
        ?datasetSourceIri ddo:has-download-url ?downloadUrl .
    }
}